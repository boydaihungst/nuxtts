{
  "name": "first-nuxt-app",
  "version": "1.0.0",
  "description": "My classy Nuxt.js project",
  "author": "Huy Hoang",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "dev:server": "cross-env NODE_ENV=development nodemon --exec nuxt-ts",
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "generate": "cross-env NODE_ENV=production nuxt-ts generate",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore . --fix"
  },
  "nodemonConfig": {
    "watch": [
      "./src/server/"
    ],
    "ext": "js,json,ts"
  },
  "dependencies": {
    "@nuxt/types": "^0.5.7",
    "@nuxt/typescript-runtime": "^0.3.3",
    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/router": "^1.5.0",
    "@types/cookie-session": "^2.0.37",
    "@types/csurf": "^1.9.36",
    "@types/lodash": "^4.14.149",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "csurf": "^1.10.0",
    "express": "^4.16.4",
    "helmet": "^3.21.2",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-typed-vuex": "^0.1.15",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.11"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.2",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^0.0.45",
    "@types/moment": "^2.13.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.16.4",
    "stylelint": "^10.1.0",
    "terser-webpack-plugin": "^2.3.1",
    "vue-class-component": "^7.1.0",
    "webpack": "^4.41.4"
  }
