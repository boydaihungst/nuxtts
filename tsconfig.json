{
  "compilerOptions": {
    "target": "es2018",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["esnext", "esnext.asynciterable", "dom"],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "removeComments": true,
    "noImplicitAny": false,
    "pretty": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxt/vue-app",
      "@nuxtjs/vuetify",
      "@nuxtjs/axios",
      "./src/@types"
    ]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
